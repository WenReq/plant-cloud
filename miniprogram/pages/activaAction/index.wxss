.page {
  width: 100vw;
  height: 100vh;
  background-color: #f7f7f7;
  text-align: center;
}
.title {
  display: block;
  font-size: 20px;
  font-weight: 600;
  color: #000;
  margin-top: 10px;
  margin-bottom: 10px;
}
.typeView image {
  line-height: 70px;
  width: 70px;
  height: 70px;
  margin-top: 30px;
}

.contentView {
    width: 100%;
    height: 40px;
    background-color: #fff;
    margin-top: 1px;
  }
  .contentView label {
    line-height: 40px;
    font-size: 14px;
    float: left;
    margin-left: 10px;
  }
  .contentView image {
    width: 15px;
    height: 15px;
    float: right;
    margin-right: 10px;
    margin-top: 12.5px;
  }
  .contentView view {
    text-align: right;
  }
  .contentView input {
    display: inline-block;
height: 40px;
font-size: 14px;
margin-right: 10px;
width: 200px;
  }

  .submit {
    background-color: #458DE2;
    color: white;
    font-weight: 600;
    width: 90%;
    margin-top: 20px;
  }
  
.rowItem {
  background-color: white;
  border-bottom: 1px solid #e4e4e4;
  padding: 12rpx;
  font-size: 28rpx;
  line-height: 40rpx;
}

.rowItemHover {
  background-color: #51DA88;
}

.showList {
  position: absolute;
  width: 86%;
  left: 56rpx;
  max-height: 600rpx;
  overflow: scroll;
  z-index: 999;
}